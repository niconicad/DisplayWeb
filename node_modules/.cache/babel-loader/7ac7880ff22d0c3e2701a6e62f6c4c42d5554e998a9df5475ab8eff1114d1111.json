{"ast":null,"code":"import ArticleCard from './ArticleCard.vue';\nexport default {\n  name: 'ArticleList',\n  components: {\n    ArticleCard\n  },\n  data() {\n    return {\n      articles: [{\n        id: 1,\n        title: '文章一',\n        author: '作者A',\n        summary: '这是文章一的简介...'\n      }, {\n        id: 2,\n        title: '文章二',\n        author: '作者B',\n        summary: '这是文章二的简介...'\n      }\n      // 更多文章...\n      ],\n      searchQuery: '',\n      filteredArticles: [],\n      currentPage: 1,\n      articlesPerPage: 5\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.filteredArticles.length / this.articlesPerPage);\n    },\n    paginatedArticles() {\n      const start = (this.currentPage - 1) * this.articlesPerPage;\n      const end = start + this.articlesPerPage;\n      return this.filteredArticles.slice(start, end);\n    }\n  },\n  created() {\n    this.filterArticles();\n  },\n  methods: {\n    filterArticles() {\n      this.filteredArticles = this.articles.filter(article => article.title.includes(this.searchQuery) || article.author.includes(this.searchQuery) || article.summary.includes(this.searchQuery));\n      this.currentPage = 1; // 重置到第一页\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["ArticleCard","name","components","data","articles","id","title","author","summary","searchQuery","filteredArticles","currentPage","articlesPerPage","computed","totalPages","Math","ceil","length","paginatedArticles","start","end","slice","created","filterArticles","methods","filter","article","includes","nextPage","prevPage"],"sources":["D:\\HuaweiMoveData\\Users\\13565650660\\Desktop\\WebSite\\display_web\\src\\components\\ArticleDetail.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <input v-model=\"searchQuery\" @input=\"filterArticles\" placeholder=\"搜索文章...\" />\r\n    <div class=\"article-list\">\r\n      <ArticleCard\r\n        v-for=\"article in paginatedArticles\"\r\n        :key=\"article.id\"\r\n        :id=\"article.id\"\r\n        :title=\"article.title\"\r\n        :author=\"article.author\"\r\n        :summary=\"article.summary\"\r\n      />\r\n    </div>\r\n    <div class=\"pagination\">\r\n      <button @click=\"prevPage\" :disabled=\"currentPage === 1\">上一页</button>\r\n      <span>第 {{ currentPage }} 页 / 共 {{ totalPages }} 页</span>\r\n      <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">下一页</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ArticleCard from './ArticleCard.vue';\r\n\r\nexport default {\r\n  name: 'ArticleList',\r\n  components: {\r\n    ArticleCard\r\n  },\r\n  data() {\r\n    return {\r\n      articles: [\r\n        { id: 1, title: '文章一', author: '作者A', summary: '这是文章一的简介...' },\r\n        { id: 2, title: '文章二', author: '作者B', summary: '这是文章二的简介...' }\r\n        // 更多文章...\r\n      ],\r\n      searchQuery: '',\r\n      filteredArticles: [],\r\n      currentPage: 1,\r\n      articlesPerPage: 5\r\n    }\r\n  },\r\n  computed: {\r\n    totalPages() {\r\n      return Math.ceil(this.filteredArticles.length / this.articlesPerPage);\r\n    },\r\n    paginatedArticles() {\r\n      const start = (this.currentPage - 1) * this.articlesPerPage;\r\n      const end = start + this.articlesPerPage;\r\n      return this.filteredArticles.slice(start, end);\r\n    }\r\n  },\r\n  created() {\r\n    this.filterArticles();\r\n  },\r\n  methods: {\r\n    filterArticles() {\r\n      this.filteredArticles = this.articles.filter(article =>\r\n        article.title.includes(this.searchQuery) ||\r\n        article.author.includes(this.searchQuery) ||\r\n        article.summary.includes(this.searchQuery)\r\n      );\r\n      this.currentPage = 1; // 重置到第一页\r\n    },\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    },\r\n    prevPage() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.article-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 10px;\r\n}\r\n.pagination {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n}\r\n.pagination button {\r\n  margin: 0 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAsBA,OAAOA,WAAU,MAAO,mBAAmB;AAE3C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,KAAK;QAAEC,MAAM,EAAE,KAAK;QAAEC,OAAO,EAAE;MAAc,CAAC,EAC9D;QAAEH,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,KAAK;QAAEC,MAAM,EAAE,KAAK;QAAEC,OAAO,EAAE;MAAc;MAC7D;MAAA,CACD;MACDC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,EAAE;MACpBC,WAAW,EAAE,CAAC;MACdC,eAAe,EAAE;IACnB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACN,gBAAgB,CAACO,MAAK,GAAI,IAAI,CAACL,eAAe,CAAC;IACvE,CAAC;IACDM,iBAAiBA,CAAA,EAAG;MAClB,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACR,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,eAAe;MAC3D,MAAMQ,GAAE,GAAID,KAAI,GAAI,IAAI,CAACP,eAAe;MACxC,OAAO,IAAI,CAACF,gBAAgB,CAACW,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IAChD;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACPD,cAAcA,CAAA,EAAG;MACf,IAAI,CAACb,gBAAe,GAAI,IAAI,CAACN,QAAQ,CAACqB,MAAM,CAACC,OAAM,IACjDA,OAAO,CAACpB,KAAK,CAACqB,QAAQ,CAAC,IAAI,CAAClB,WAAW,KACvCiB,OAAO,CAACnB,MAAM,CAACoB,QAAQ,CAAC,IAAI,CAAClB,WAAW,KACxCiB,OAAO,CAAClB,OAAO,CAACmB,QAAQ,CAAC,IAAI,CAAClB,WAAW,CAC3C,CAAC;MACD,IAAI,CAACE,WAAU,GAAI,CAAC,EAAE;IACxB,CAAC;IACDiB,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACjB,WAAU,GAAI,IAAI,CAACG,UAAU,EAAE;QACtC,IAAI,CAACH,WAAW,EAAE;MACpB;IACF,CAAC;IACDkB,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAAClB,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDepende